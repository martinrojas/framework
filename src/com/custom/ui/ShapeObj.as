package com.custom.ui {		import flash.display.MovieClip;	import flash.display.Shape;	import flash.display.Graphics;	import flash.events.*;	import flash.text.*;		public class ShapeObj extends MovieClip {				private var shapeWidth:uint;		private var shapeHeight:uint;		private var cornerRadius:Number;		private var shapeColor:uint;		private var shapeOpacity:Number;		private var shape:String;				private var newShape:Shape = new Shape;				public function ShapeObj(_shapeWidth:uint, _shapeHeight:uint, _shapeColor:uint, _radius:Number = 0, _shapeOpacity:Number = 1, _shape:String = "round"):void {						shapeWidth = _shapeWidth;			shapeHeight = _shapeHeight;			cornerRadius = _radius;			shapeColor = _shapeColor;			shapeOpacity = _shapeOpacity;			shape = _shape;			buildShape(shape);		}				private function buildShape(type:String):void {			switch (type) {			case "round":				newShape.graphics.beginFill(shapeColor, shapeOpacity);				newShape.graphics.drawEllipse(0, 0, shapeWidth, shapeHeight);				newShape.graphics.endFill();				addChild(newShape);				break;			case "rect":				newShape.graphics.beginFill(shapeColor, shapeOpacity);				newShape.graphics.drawRect(0, 0, shapeWidth, shapeHeight);				newShape.graphics.endFill();				addChild(newShape);				break;			case "roundRect":				newShape.graphics.beginFill(shapeColor, shapeOpacity);				newShape.graphics.drawRoundRect(0, 0, shapeWidth, shapeHeight, cornerRadius, cornerRadius);				newShape.graphics.endFill();				addChild(newShape);				break;			}		}	}}